<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="483.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HistoryViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="32.0" prefWidth="191.0">
         <children>
            <Label text="Temperature History" />
         </children>
      </VBox>
      <VBox prefHeight="374.0" prefWidth="399.0">
         <children>
            <TableView fx:id="temperatureTable" prefHeight="371.0" prefWidth="399.0">
              <columns>
                <TableColumn fx:id="IDColumn" prefWidth="85.0" text="Thermometer" />
                  <TableColumn fx:id="temperatureColumn" prefWidth="82.0" text="Temperature" />
                  <TableColumn fx:id="timeColumn" prefWidth="195.0" text="Time" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <VBox alignment="BOTTOM_RIGHT">
         <children>
            <Button mnemonicParsing="false" onAction="#backOnAction" text="Back" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</VBox>
